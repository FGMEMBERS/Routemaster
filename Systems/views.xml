<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <chase-distance-m type="double" archive="y"> -30 </chase-distance-m>

    <!-- <name>cannonveiw</name> -->

    <view n="100">
    <name>Cannon operator View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>

      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <heading-offset-deg> 180 </heading-offset-deg>
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <x-offset-m archive="y" type="double">  0.0073 </x-offset-m> <!-- right -->
      <y-offset-m archive="y" type="double">  2.46 </y-offset-m> <!--    up -->
      <z-offset-m archive="y" type="double"> 0.48 </z-offset-m> <!--  back -->
      </config>
  </view>
  
  <view n="101">
    <name>cannon view</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <heading-offset-deg> 180 </heading-offset-deg>
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <x-offset-m archive="y" type="double">  0.0001 </x-offset-m> <!-- right -->
      <y-offset-m archive="y" type="double">  3.25 </y-offset-m> <!--    up -->
      <z-offset-m archive="y" type="double"> 1.28 </z-offset-m> <!--  back -->
    </config>
    </view>

 
  <view n="105">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 30 </default-field-of-view-deg>
      <x-offset-m archive="y"> 0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y"> 3.1422 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> -4.895 </z-offset-m> <!--Back-->
    </config>
  </view>

  <view n="0">
  <name>Driver's View</name>
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y" type="double">  0.713 </x-offset-m> <!-- right -->
      <y-offset-m archive="y" type="double">  2.664 </y-offset-m> <!--    up -->
      <z-offset-m archive="y" type="double"> -1.689 </z-offset-m> <!--  back -->
	<pitch-offset-deg archive="y">0</pitch-offset-deg>
      <default-field-of-view-deg> 80 </default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="7">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

</PropertyList>
