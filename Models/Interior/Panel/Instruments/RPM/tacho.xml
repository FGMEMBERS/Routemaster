<?xml version="1.0"?>

<PropertyList>

 <path>tacho.ac</path>

<effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>Glass</object-name>  
    </effect>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>PointerTip</object-name>   
  <emission>
   <red>0.68</red>
   <green>0.62</green>
   <blue>0.62</blue>
   <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>


 <animation> 
  <type>rotate</type>
  <object-name>PointerCentre</object-name>
  <object-name>PointerTip</object-name>
  <property>engines/engine/rpm</property>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>1290</ind><dep>90</dep></entry>
   <entry><ind>1900</ind><dep>180</dep></entry>
   <entry><ind>2530</ind><dep>270</dep></entry>
   <entry><ind>3200</ind><dep>360</dep></entry>
  </interpolation>
  <axis> 
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>         
 
</PropertyList>
